.setting-box.hide-setting
    .toggle-setting
        i.fas.fa-cog
    .link-box.color-options.text-center
        h4.option-title backgroung settings
        -var themes = ["green-theme","blue-theme","red-theme","purple-theme","yellow-theme"]
        ul.list-unstyled
            each theme in themes 
                li(data-color = theme)
    .link-box.text-center
        h4.option-title color settings
    .link-box.text-center
        h4.option-title font settings